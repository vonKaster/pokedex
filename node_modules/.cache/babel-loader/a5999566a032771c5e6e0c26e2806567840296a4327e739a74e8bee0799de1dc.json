ult {\n  name: \"Pokedex\",\n\n  data() {\n    return {\n      pokemonModel: \"\",\n      timer: \"\",\n      pokemon: {\n        id: \" \",\n        name: \"\",\n        img: \"../assets/img/pokeball.png\",\n      },\n    };\n  },\n\n  methods: {\n    obtenerPokemon(id) {\n      axios\n        .get(`https://pokeapi.co/api/v2/pokemon/${id}`)\n        .then((response) => {\n          console.log(response);\n          this.pokemon.img = response.data.sprites.front_default;\n          this.pokemon.name = response.data.name;\n          this.pokemon.id = response.data.id;\n        })\n        .catch((error) => {\n          this.pokemon.name = \"No existe\";\n        });\n    },\n    getRandomInt() {\n      return Math.floor(Math.random() * 906);\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}