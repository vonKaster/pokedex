ed: {\n    localData() {\n      return store.localData;\n    }\n  },\n\n\n  created: function () {\n    let coins = JSON.parse(localStorage.getItem(\"coins\"));\n    if (coins === null) {\n      this.coins = 0;\n    } else {\n      this.coins = coins;\n    }\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}