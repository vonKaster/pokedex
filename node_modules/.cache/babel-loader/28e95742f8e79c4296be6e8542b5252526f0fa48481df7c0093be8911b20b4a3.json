{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"Pokedex\",\n  data() {\n    return {\n      disabled: false,\n      timeout: null,\n      pokemonModel: \"\",\n      timer: \"\",\n      counter: \"5\",\n      pokemonsOwned: \"\",\n      pokemon: {\n        id: \" \",\n        name: \"\",\n        img: \"https://www.pngitem.com/pimgs/m/580-5807856_pokemon-pokeball-pokeball-transparent-background-hd-png-download.png\"\n      }\n    };\n  },\n  methods: {\n    obtenerPokemon(id) {\n      axios.get(`https://pokeapi.co/api/v2/pokemon/${id}`).then(response => {\n        console.log(response);\n        this.pokemon.img = response.data.sprites.front_default;\n        this.pokemon.name = response.data.name;\n        this.pokemon.id = response.data.id;\n      }).catch(error => {\n        this.pokemon.name = \"No existe\";\n      });\n    },\n    getRandomInt() {\n      return Math.floor(Math.random() * 906);\n    },\n    delay(id) {\n      this.disabled = true;\n      if (this.counter > 0) {\n        this.timeout = setTimeout(() => {\n          this.disabled = false;\n          this.counter--;\n          this.delay();\n        }, 1000);\n      }\n      this.obtenerPokemon(id);\n    }\n  }\n};","map":{"version":3,"mappings":"AAuBA;AACA;EACAA;EAEAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;QACAC;QACAT;QACAU;MACA;IACA;EACA;EAEAC;IACAC;MACAC,MACAC,+CACAC;QACAC;QACA;QACA;QACA;MACA,GACAC;QACA;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;MAEA;QACA;UACA;UACA;UACA;QACA;MACA;MAEA;IACA;EACA;AACA","names":["name","data","disabled","timeout","pokemonModel","timer","counter","pokemonsOwned","pokemon","id","img","methods","obtenerPokemon","axios","get","then","console","catch","getRandomInt","delay"],"sourceRoot":"src/components","sources":["Pokedex.vue"],"sourcesContent":["<template>\n  <div class=\"container text-center\">\n    <v-text-field\n      v-model=\"pokemonModel\"\n      label=\"Pokemon (Nombre o ID)\"\n    ></v-text-field>\n    <v-btn :disabled=\"disabl